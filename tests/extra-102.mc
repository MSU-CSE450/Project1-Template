SET radius = 3.5
SET PI = 3.14
/* And now that we have the input values.
   we will use them to calculate the area. */
SET r_squ = /* IGNORE THIS! */ radius * radius
SET area = PI * r_squ
/* Print the calculated area on the next line */
PRINT "area = " area

